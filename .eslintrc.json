{
  "env": {
    "browser": true,
    "es6": true,
    "cypress/globals": true,
    "node": true
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "cypress"]
}
